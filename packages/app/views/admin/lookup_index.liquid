{% layout "layouts/base_admin.liquid" %}

{% block content %}
  <h1>lookup</h1>

  <form method="post" action="/admin/lookup/synchronize/list">
    <button
      type="submit"
      name="action"
      value="synchronize">synchronize:list</button>
  </form>

  <form method="post" action="/admin/lookup/synchronize/url">
    <input
      type="text"
      name="functionName"
      value="ayane-dev-http"
      placeholder="sample-function-name" />

    <button
      type="submit"
      name="action"
      value="synchronize">synchronize:url</button>
  </form>

  <form method="post" action="/admin/lookup/synchronize/event">
    <input
      type="text"
      name="functionName"
      value="miyako-dev-sqsSimple"
      placeholder="sample-function-name" />

    <button type="submit">synchronize:event</button>
  </form>
{% endblock %}
